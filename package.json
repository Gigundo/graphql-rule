{
  "name": "graphql-model",
  "version": "0.0.6",
  "description": "A data model class for wrapping a data object for easier resolving fields in GraphQL",
  "main": "lib/index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "build": "babel src --out-dir lib",
    "build-watch": "babel src --watch --out-dir lib",
    "start": "npm run build && npm run test-build && npm run test",
    "test-build": "babel test --out-dir testout",
    "test-build-watch": "babel test --watch --out-dir testout",
    "test": "mocha testout/**/*.js",
    "test-watch": "mocha --watch testout/**/*.js",
    "watch": "npm run build-watch & npm run test-build-watch & npm run test-watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/joonhocho/graphql-model.git"
  },
  "keywords": [
    "graphql",
    "data",
    "model",
    "wrapper",
    "resolve",
    "fields"
  ],
  "author": "Joon Ho Cho",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/joonhocho/graphql-model/issues"
  },
  "homepage": "https://github.com/joonhocho/graphql-model#readme",
  "devDependencies": {
    "babel-cli": "^6.8.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-stage-0": "^6.5.0",
    "chai": "^3.5.0",
    "chai-as-promised": "^5.3.0",
    "mocha": "^2.4.5"
  }
}
